{
  "attributes": {
    "h5p": {
      "required": [
        "data-resource",
        "data-path"],
      "optional": [
        [ "data-title" ],
        [ "data-url" ]
      ]
    },
    "external": {
      "required": [
        "data-resource",
        "data-url"
      ],
      "optional": [
        [ "data-caption" ],
        [ "data-title" ]
      ]
    },
    "error": {
      "required": [
        "data-resource",
        "data-message"
      ]
    },
    "audio": {
      "required": [
        "data-resource",
        "data-resource_id",
        "data-type"
      ],
      "optional": [
        [ "data-caption" ]
      ]
    },
    "nrk": {
      "required": [
        "data-resource",
        "data-nrk-video-id",
        "data-url"
      ]
    },
    "content-link": {
      "required": [
        "data-resource",
        "data-content-id",
        "data-link-text"
      ],
      "optional": [
        [ "data-open-in" ],
        [ "data-content-type" ]
      ]
    },
    "brightcove": {
      "required": [
        "data-resource",
        "data-caption",
        "data-videoid",
        "data-account",
        "data-player"
      ],
      "optional": [
        [ "data-title" ]
      ]
    },
    "image": {
      "required": [
        "data-resource",
        "data-resource_id",
        "data-size",
        "data-alt",
        "data-caption",
        "data-align"
      ],
      "optional": [
        [
          "data-upper-left-x",
          "data-upper-left-y",
          "data-lower-right-x",
          "data-lower-right-y"
        ], [
          "data-focal-x",
          "data-focal-y"
        ]
      ]
    },
    "concept": {
      "required": [
        "data-resource",
        "data-content-id",
        "data-link-text",
        "data-type"
      ]
    },
    "iframe": {
      "required": [
        "data-resource",
        "data-url",
        "data-width",
        "data-height"
      ],
      "validSrcDomains": [
        ".*.ndla.no",
        "vimeo.com",
        "ndla.filmiundervisning.no",
        "prezi.com",
        "commoncraft.com",
        "embed.kahoot.it",
        "fast.wistia.com",
        ".*.khanacademy.org",
        "khanacademy.org",
        "tv2skole.no",
        ".*.tv2skole.no",
        "www.scribd.com",
        "www.vg.no",
        "livestream.com",
        ".*.livestream.com",
        "channel9.msdn.com",
        "tomknudsen.no",
        "www.tomknudsen.no",
        "geogebra.org",
        "www.geogebra.org",
        "ggbm.at",
        "www.imdb.com",
        "imdb.com",
        "miljoatlas.miljodirektoratet.no",
        "www.miljostatus.no",
        "miljostatus.no",
        "phet.colorado.edu",
        "static.nrk.no",
        "e.issuu.com",
        "lab.concord.org",
        ".*.worldbank.org",
        "worldbank.org",
        "embed.molview.org",
        "embed.ted.com"
      ]
    },
    "footnote": {
      "required": [
        "data-resource",
        "data-title",
        "data-type",
        "data-year",
        "data-edition",
        "data-publisher",
        "data-authors"
      ]
    },
    "code-block": {
      "required": [
        "data-resource",
        "data-code-format",
        "data-code-content"
      ],
      "optional": [
        [ "data-title" ]
      ]
    },
    "related-content": {
      "mustContainAtLeastOneOptionalAttribute": true,
      "required": [
        "data-resource"
      ],
      "optional": [
        [
          "data-article-id"
        ], [
          "data-url",
          "data-title"
        ]
      ],
      "mustBeDirectChildOf": {
        "name": "div",
        "requiredAttr": [
          {
            "data-type": "related-content"
          }
        ]
      }
    },
    "file": {
      "required": [
        "data-resource",
        "data-title",
        "data-path"
      ],
      "optional": [
        [ "data-type" ],
        [ "data-alt" ],
        [ "data-display" ]
      ],
      "mustBeDirectChildOf": {
        "name": "div",
        "requiredAttr": [
          {
            "data-type": "file"
          }
        ]
      }
    }
  }
}
